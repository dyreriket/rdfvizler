@prefix gv:  <http://dyreriket.xyz/sau/vocab/core#> .
@prefix gva: <http://dyreriket.xyz/sau/vocab/attribute#> .
@prefix gvn: <http://dyreriket.xyz/sau/vocab/attribute/default/node#> .
@prefix gve: <http://dyreriket.xyz/sau/vocab/attribute/default/edge#> .

@prefix :  <http://example.com#> .

:root a gv:RootGraph , gv:DiGraph , gv:StrictGraph ;
  gva:rankdir "LR";
  gva:nodesep "0.3";
  :something "else";
  gvn:fontsize "10px";
  gvn:style "box";
  gve:style "dashed";	
  gv:hasNode :n1, :n2, :n3 ;
  gv:hasEdge :e1, :e2, :e3 ;
  gv:hasSubGraph :g1.


:n1 gv:hasID "asdf" ;
    gva:shape "box";
    gva:style "filled";
    gva:fillcolor "blue" .

:e1
  gv:hasSource :n1 ;
  gv:hasTarget :m1 ;
  gva:style "dotted".
  

:e2
  gv:hasSource :n1 ;
  gv:hasTarget :n3 ;
  gva:color "red".

:e3
  gv:hasSource :n2 ;
  gv:hasTarget :n3 .


:g1
  gv:hasID "clusterG1" ;
  gv:hasNode :m1, :m2;
  gv:hasEdge :f1 .

:f1
  gv:hasSource :m1;
  gv:hasTarget :m2.